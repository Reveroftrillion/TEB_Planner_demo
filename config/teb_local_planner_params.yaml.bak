TebLocalPlannerROS:
  odom_topic: /odom
  map_frame: map

  max_vel_x: 0.6
  max_vel_theta: 1.2
  acc_lim_x: 0.8
  acc_lim_theta: 1.5

  footprint_model:
    type: "polygon"
    vertices: [[0.30, 0.15], [0.30, -0.15], [-0.30, -0.15], [-0.30, 0.15]]

  teb_autosize: true
  dt_ref: 0.3
  dt_hysteresis: 0.1
  global_plan_overwrite_orientation: true
  allow_init_with_backwards_motion: false

  min_obstacle_dist: 0.20
  inflation_dist: 0.30

  weight_obstacle: 50.0
  weight_kinematics_forward_drive: 1.0
  weight_velocity_obstacle_ratio: 1.0
